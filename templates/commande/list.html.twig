<!-- Consultation Section Begin -->
<section class="hero spad set-bg" data-setbg="{{ asset('img/batata.jpg') }}">
    <div class="container">
        <div class="row">
            <div class="col-lg-8">
                <div class="consultation__form" >
                    <div class="section-title">
                        <h2>Mon panier</h2>
                        <table class="table table-striped table-hover">
                            <thead>
                            <tr>
                                <th> Produit</th>
                                <th></th>
                                <th> Prix
                                </th>
                                <th> Quantite </th>
                                <th> Total </th>
                                <th> </th>
                                <th> </th>
                                <th> </th>
                            </tr>
                            </thead>
                            <tbody>
                            {% for commande in commande %}
                                {% for produit in produits %}

                                    {% if commande.produit==produit %}
                                        <tr>

                                        <td><img class="img-fluid rounded" src="{{ produit.image }}" alt="Image de {{ produit.nomP }}" width="100" height="50"></td>
                                        <td>{{ produit.nomP }} </td>
                                        <td>{{ produit.prix }} €</td>
                                        <td>
                                            {{ commande.quantite }}
                                        </td>

                                        <td>{{produit.prix * commande.quantite  }} </td>
                                        <td>
{#
                                                   {{ render(controller(
                                            'App\\Controller\\CommandeController::updateAction',
                                                     { 'id' : commande  }
                                            )) }}
#}

                                        </td>
                                        <td>
                                            <a href="/mod/{{ panier.id }}/{{ commande.id }}/{{ produit.id }}" >
                                                <button type="button" class="btn btn-warning">Modifier</button>
                                            </a>
                                        </td>
                                        <td>
                                        <a href="{{ path('panier_sup' ,{'id':client.id, 'id1':panier.id ,'id2':commande.id }) }}" >
                                            <button type="button" class="btn btn-danger">Supprimer</button>
                                        </a>
                                        </td>{% endif %}

                                    </tr>
                                    {% endfor %}
                            {% endfor %}

                            </tbody>
                        </table>

                        <dl class="dl-horizontal pull-right">

                                {% set total = 0 %}
                                {% for commande in commande %}
                                    {% for produit in produits %}
                                        {% if commande.produit==produit %}
                                {% set total = total + ( produit.prix * commande.quantite ) %}
                                {% endif %}
                                    {% endfor%}
                                {% endfor %}
                            <dt>Total HT :</dt><dd>{{ total }} €</dd>





                        </dl>


                        <div class="clearfix"></div>
                        <a href="#" class="btn btn-success pull-right">Payer</a>

                    </div>











                </div>
            </div>

        </div>
    </div>

</section>
