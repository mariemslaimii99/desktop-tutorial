        <div class="page-title">
            <h1>List des reservations</h1>
            <div class="row">
                <div class="col-md-12 col-lg-12">
                    <div class="card">
                        <div class="card-header">Reservation Table</div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                    <tr>
                                        <th>Nom</th>
                                        <th>Prenom</th>
                                        <th>Nom Event</th>
                                        <th>Endroit</th>
                                        <th>Date Depart</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        {% for Reservation in reservation %}
                                        {% for Event in Event %}
                                        {% if Event.id==Reservation.idevent %}
                                        {% for clients in clients %}
                                        {% if clients.id==Reservation.idclient %}

                                    <tr>
                                        <td>{{ clients.nom}}</td>
                                        <td>{{ clients.prenom }}</td>
                                        <td>{{ Event.NomEvent}}</td>
                                        <td>{{ Event.nomEndroit}}</td>
                                        <td>{{ Event.DateDepart|date('Y-m-d')}}</td>


                                        <td>
                                            <form action="{{ path('delete23',{ 'id':Reservation.id }) }}">
                                                <button class="btn btn-danger mb-2"
                                                        onclick="return confirm('Etes-vous sûr de supprimer cet event?');" >Supprimer</button>
                                            </form>
                                        </td>
                                    </tr>
                                    {% endif %}
