{% extends 'base.html.twig' %}
{% block title%} Liste des produits{% endblock %}

{% block body %}
    {% if products %}
        <table id="products" class="table table-striped">
            <thead>
            <tr>

                <th>Nom</th>
                <th>Prix</th>
                <th>Categorie</th>
                <th>description</th>
                <th>Image</th>
            </tr>
            </thead>
            <tbody>
            {% for product in products %}
                <tr>

                    <td>{{ product.nomP }}</td>
                    <td>{{ product.prix }}</td>
                    <td>{{ product.categorie }}</td>
                    <td>{{ product.description }}</td>
                    <td>{{ product.image }}</td>
                    <td>

                    <td><a href="{{ path('show', {'id': product.id}) }}" class="btn btn-dark">Details</a></td>
                    <td><a href="{{ path('modify_product', {'id': product.id}) }}" class="btn btn-dark">Modifier</a></td>

                    <td><a href="{{ path('delete_product', {'id': product.id}) }}" class="btn btn-dark"
                           onclick="return confirm('Etes-vous sÃ»r de supprimer cet article?');">Supprimer</a>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>Aucun articles</p>
    {% endif %}
    <td><a href="{{ path('add_product') }}"class="btn btn-dark">ajouter produit</a></td>
{% endblock %}

