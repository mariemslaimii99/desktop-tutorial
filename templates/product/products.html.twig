        <div class="page-title">
            <h1>List des produits</h1>
            <div class="row">
                <div class="col-md-12 col-lg-12">
                    <div class="card">
                        <div class="card-header">Product Table</div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                    <tr>
                                        <th>nomP</th>
                                        <th>description</th>
                                        <th>image</th>
                                        <th>prix</th>
                                        <th>categorie</th>

                                        <th>detail</th>
                                        <th>modifier</th>
                                        <th>supprimer</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        {% for product in products %}
                                    <tr>

                                        <td>{{ product.nomP }}</td>
                                        <td>{{ product.description }}</td>

                                        <td>{{ product.image }}</td>

                                        <td>{{ product.prix }}</td>
                                        <td> {% for categorie in categorie %}
                                            {% if product.categorie==categorie %}
                                                <span>{{ categorie.nom }}</span>
                                            {% endif %}
                                        {% endfor %}
                                        </td>
                                        <td>
                                            <form action="{{ path('show',{ 'id':product.id }) }}">
                                                <button class="btn btn-success mb-2">Details</button>
                                            </form></td>

                                        <td>  <form action="{{ path('modify_product',{ 'id':product.id }) }}">
                                                <button class="btn btn-primary mb-2" >Modifier</button>
                                            </form></td>

                                        <td>
                                            <form action="{{ path('delete_product',{ 'id':product.id }) }}">
                                                <button class="btn btn-danger mb-2"
                                                        onclick="return confirm('Etes-vous sûr de supprimer cet event?');" >Supprimer</button>
                                            </form>
                                        </td>
                                    </tr>
                                    {% endfor %}
