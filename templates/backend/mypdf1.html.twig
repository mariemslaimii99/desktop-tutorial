<h1 >Views Report : </h1>
<table id="customers">
    <thead>
    <tr>
        <th>View name</th>
        <th>View date</th>


    </tr>
    </thead>
    <tbody>
    {% for view in views %}
        <tr>
            <td>{{ view.routename }}</td>
            <td>{{ view.visitdate|format_datetime() }}</td>

        </tr>
    {% endfor %}
    </tbody>

</table>

