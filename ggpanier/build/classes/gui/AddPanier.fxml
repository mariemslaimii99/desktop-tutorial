<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AddPanierController">
   <children>
      <ImageView fitHeight="736.0" fitWidth="1197.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/fond.jpg" />
         </image>
         <effect>
            <DropShadow />
         </effect>
      </ImageView>
      <TableView fx:id="tab" layoutX="179.0" layoutY="156.0" onMouseClicked="#SetValue" prefHeight="295.0" prefWidth="435.0">
        <columns>
          <TableColumn fx:id="idtab" prefWidth="131.0" text="id" />
          <TableColumn fx:id="nomtab" prefWidth="158.0" text="Nom" />
            <TableColumn fx:id="prixtab" prefWidth="143.0" text="Prix" />
        </columns>
      </TableView>
      <Button fx:id="ajouter" layoutX="676.0" layoutY="451.0" mnemonicParsing="false" onAction="#AjouterPanier" text="ajouter au panier" />
      <Button fx:id="AfficherPanier" layoutX="1031.0" layoutY="37.0" mnemonicParsing="false" onAction="#AfficherPanier" text="Afficher Panier" />
   </children>
</AnchorPane>
